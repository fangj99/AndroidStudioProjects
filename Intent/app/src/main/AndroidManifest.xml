<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lance.intent" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OtherActivity"
            android:label="@string/title_activity_other" >
            <intent-filter>
                <action android:name = "com.example.lance"/>
                <action android:name = "com.example.lancefang"/>
                <category android:name="com.example.category.java"/>
                <category android:name="com.example.category.php"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="lance" android:host="www.martnyc.com" android:path="/web"/>
                <data android:mimeType="image/*"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
